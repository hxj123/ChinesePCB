<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.administrator.myapplication">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.GET_ACCOUNTS" />
    <uses-permission android:name="android.permission.READ_PROFILE" />
    <uses-permission android:name="android.permission.READ_CONTACTS" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/logo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".home.Search" />
        <activity android:name=".personCenter.UserInfo" />
        <activity android:name=".home.ProgramList" />
        <activity
            android:name=".home.ProgramInfo"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".square.ArticleInfo" />
        <activity android:name=".square.CreateArticle" />
        <activity android:name=".home.MyProgramList" />
        <activity android:name=".personCenter.ItemReview" />
        <activity android:name=".personCenter.ExchangeService" />
        <activity android:name=".square.SellProgram"></activity>
    </application>

</manifest>